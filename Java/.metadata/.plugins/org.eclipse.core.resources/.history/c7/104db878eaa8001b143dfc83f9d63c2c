package Java;
//Access Modifiers determines whether the other classes can use particular property or not
/*
  
Modifier        class 				Package					subclass     				world
---------------------------------------------------------------------------------------------------
public				Y					Y						Y						Y
protected		    Y					Y						Y						N
default(no)			Y					Y						N						N
private			    Y					N						N						N

*/


class Modifier{
	public int x=5;
	protected int y=6;
	int z=7;
	private int a=8;
}
public class AY_AccessModifiers {

public static void main(String[] args) {
		
	}
}


package Java;

class MyEmp{
	
	public MyEmp() {
		id =45;
		name="default";
	}
	private int id;
	private String name;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
}
public class AN_Constructors_in_Java {	
	
	public static void main(String[] args) {
		MyEmp e1 = new MyEmp();
		//e1.setName("Kishan");
		System.out.println(e1.getName());
	}
}


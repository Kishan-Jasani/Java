package Java;

class Employee{
	int id;
	String name;
}

public class AM_AccessModifier_GetterSetter {	
	//Access Modifier:-
	//Private - Not directly accessible in other class
	//Public - accessible everywhere
	//Default
	//Protected
	
	public static void main(String[] args) {
		Employee em1 = new Employee();
		em1.id=9025;
		em1.name="Kishan";
		
	}
}


package Java;

public class AC_Strings {
	public static void main(String[] args) {
		System.out.println("Kishan");       //Cursor will go in new line after print
		
		System.out.print("Kishan ");			//Cursor will remain a same line
		
		int a =6;
		float b =5.6f;
		String c = "Heyy";
		System.out.printf("Value of a is %d and value of b is %f and value of c is %s", a,b,c);
		
		System.out.format("Value of a is %d and value of b is %f and value of c is %s", a,b,c);
	}
}
